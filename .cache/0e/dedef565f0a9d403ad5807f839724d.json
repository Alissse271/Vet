ack-text: #080606;\r\n$brown: #59443F;\r\n\r\n// Адаптивный шрифт\r\n$maxWidth: 1920;\r\n@mixin adaptiv-font($pcSize, $mobSize) {\r\n $addSize: $pcSize - $mobSize;\r\n $maxWidth: $maxWidth - 320;\r\n font-size: calc(#{$mobSize + px} + #{$addSize} * ((100vw - 320px) / #{$maxWidth}));\r\n}\r\n\r\n%tplstyle-sect-about-comp {\r\n    font-weight: 700;\r\n    // font-size: 20px;\r\n    @include adaptiv-font (20, 12);\r\n    line-height: 100%;\r\n}\r\n\r\n","style.scss":"@import 'variables.scss';\r\n\r\nbody {\r\n    font-family: 'Futura New', sans-serif;\r\n    max-width: 1920px;\r\n    margin: 0 auto;\r\n}\r\n\r\nsection {\r\n    margin: 0 auto;\r\n}\r\n\r\n.header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    max-width: 1760px;\r\n    margin: 65px 80px 0;\r\n\r\n    > a {\r\n        pointer-events: none;\r\n        cursor: default;\r\n        max-width: 100px;\r\n        max-height: 100px;\r\n    \r\n        img {\r\n            width: 100%;\r\n            height: 100%;\r\n        }\r\n    }\r\n\r\n    &-nav {\r\n        display: flex;\r\n        align-items: center;\r\n\r\n        a {\r\n            @extend %tplstyle-sect-about-comp;\r\n            color: $brown-light;\r\n\r\n            &:not(:last-child) {\r\n                padding-right: 80px;\r\n            }\r\n        }\r\n    }\r\n\r\n    &-location {\r\n        display: flex;\r\n        align-items: center;\r\n\r\n        img {\r\n            width: 46px;\r\n            height: 46px;\r\n        }\r\n\r\n        p {\r\n            @extend %tplstyle-sect-about-comp;\r\n            color: $orange;\r\n        }\r\n    }\r\n}\r\n\r\n.about-comp-section {\r\n    display: flex;\r\n    max-width: 1920px;\r\n\r\n    .nav {\r\n        width: 40px;\r\n        margin-left: 110px;\r\n        margin-top: 130px;\r\n\r\n        &-number {\r\n\r\n            a {\r\n                @extend %tplstyle-sect-about-comp;\r\n            font-size: 20px;\r\n            color: $brown-light;\r\n            }\r\n        \r\n            &:not(:last-child) {\r\n                padding: 0 14px 70px;\r\n            }\r\n\r\n            &:last-child {\r\n                padding: 0 14px 10px;\r\n            }\r\n\r\n            &-active {\r\n                width: 40px;\r\n                height: 40px;\r\n                background-image: url(\"img/nav-ellipse.svg\");\r\n                margin-bottom: 60px;\r\n                padding: 10px 14px;\r\n\r\n                a {\r\n                @extend %tplstyle-sect-about-comp;\r\n                font-size: 20px;\r\n                color: $black-text;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    .about-comp-info {\r\n        display: flex;\r\n        margin-left: 265px / 1840px * 100%;  \r\n\r\n        .block-comp-info {\r\n            position: relative;\r\n            max-width: 645px;\r\n            margin-top: 151px;\r\n\r\n            .comp-name {\r\n                font-family: \"SchoolBook Cyrillic\", sans-serif;\r\n                // font-size: 80px;\r\n                @include adaptiv-font (80, 24);\r\n                line-height: 138%;\r\n            }\r\n\r\n            > a {\r\n                display: inline-block;\r\n                margin-top: 50px;\r\n                margin-bottom: 197px;\r\n                background: $orange;\r\n                box-shadow: 0px 4px 20px $orange;\r\n                border-radius: 33.5px;\r\n                min-height: 67px;\r\n                @extend %tplstyle-sect-about-comp;\r\n                font-size: 20px;\r\n                color: #fff;\r\n                padding: 24px 77px;\r\n                max-width: 285px;\r\n                text-align: center;\r\n            }\r\n\r\n            .comp-info-social {\r\n                margin-bottom: 80px;\r\n\r\n                a {\r\n                    width: 40px;\r\n                    height: 40px; \r\n\r\n                    // img {\r\n                    //     width: 100%;\r\n                    // }\r\n 